    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "mimmi20/coding-standard": "^3.0.19",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.9.1",
