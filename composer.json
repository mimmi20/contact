    "phpstan/phpstan-phpunit": "^1.2.2",
    "phpunit/phpunit": "^9.5.26"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Mimmi20\\Contact\\": "src/"
    }
